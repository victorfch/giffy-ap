{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "settings": {
      "react": {
        "version": "17.0.0"
      }
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
